#define arqu "endere√ßo"
int main (){
	arquivo = fopen(arq, "w");
   
   if (arquivo == NULL) {
      printf("Erro ao abrir o arquivo\n");
      return 1;
   }else{
   fprintf(arquivo, "Texto a ser escrito\n");
   fclose(arquivo);
   }
}}
